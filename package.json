    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.2.1",
    "archiver": "^4.0.1",
    "cache-manager": "^3.3.0",
    "cheerio": "^1.0.0-rc.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "file-type": "^15.0.1",
    "helmet": "^5.0.2",
    "mongoose": "^6.3.3",
