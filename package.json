    "@nestjs/testing": "^8.4.4",
    "@types/archiver": "^3.1.0",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.47",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.159",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.10",
